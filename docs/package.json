{
  "name": "iata-utils",
  "version": "1.0.0",
  "description": "Various IATA utilities",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/bundle.js*",
    "lint": "./node_modules/.bin/eslint *.js \"src/*.js\" \"src/*.jsx\"",
    "lint-fix": "./node_modules/.bin/eslint --fix *.js \"src/*.js\" \"src/*.jsx\"",
    "dev": "webpack-dev-server --open --inline --history-api-fallback --config webpack.dev.js",
    "build-dev": "webpack --progress --colors --config webpack.dev.js",
    "build-prod": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production webpack -p --progress --colors --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benct/iata-utils.git"
  },
  "keywords": [
    "iata",
    "optd",
    "util",
    "utilities",
    "search",
    "ben",
    "tomlin"
  ],
  "author": "Ben Christopher Tomlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benct/iata-utils/issues"
  },
  "homepage": "https://benct.github.io/iata-utils",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "core-js": "^3.37.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "regenerator-runtime": "^0.14.1"
  }
}
